    "num=['-','\\\\','(','@','?','>','&','%','/']\n",
    "n=input()\n",
    "while n!='#':\n",
    "    result=0\n",
    "    for i in  range(len(n)):\n",
    "        if num.index(n[len(n)-1-i])==8:\n",
    "            result+=-1*pow(8,i)\n",
    "        else:\n",
    "            result+=num.index(n[len(n)-1-i])*pow(8,i)\n",
    "    print(result)\n",
    "    n=input()"
