    "n=int(input())\n",
    "a=\"%.300f\"%float(1/(2**n))\n",
    "end=len(a)\n",
    "#쓸모없는 0 제거\n",
    "for i in range(end-1,1,-1):\n",
    "    if a[i]!='0':\n",
    "        end=i\n",
    "        break\n",
    "    \n",
    "print(a[:end+1])"
   ]
  }
