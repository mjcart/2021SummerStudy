    "#최대공약수\n",
    "def gcd(m,n):\n",
    "    if n==0:\n",
    "        return m\n",
    "    mod=m%n\n",
    "    if mod!=0:\n",
    "        m,n=n,mod\n",
    "        return gcd(m,n)\n",
    "    else:\n",
    "        return n\n",
    "k=int(input())\n",
    "for i in range(k):\n",
    "    x,y=map(int,input().split())\n",
    "    x,y=max(x,y),min(x,y)\n",
    "    result=x*y//gcd(x,y)\n",
    "    print(result)"
   ]
  }
